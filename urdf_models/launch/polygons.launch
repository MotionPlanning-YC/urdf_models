<?xml version="1.0"?>
<launch>
 
  <!-- send robot urdf to param server -->
 
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" default="worlds/empty.world"/>
    <arg name="paused" value="true"/>
    <arg name="gui" value="true"/>
  </include>
  
  <include file="$(find urdf_models)/launch/1_poly.launch">
    <arg name="x" value="0"/>
    <arg name="y" value="0.3"/>
    <arg name="z" value="0.04"/>
  </include>
  
  <include file="$(find urdf_models)/launch/2_poly.launch">
    <arg name="x" value="0"/>
    <arg name="y" value="0.3"/>
    <arg name="z" value="0.12"/>
  </include>


</launch>
